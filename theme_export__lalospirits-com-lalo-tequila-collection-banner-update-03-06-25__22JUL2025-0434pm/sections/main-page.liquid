<div class="container">
  <div class="mx-auto mt-8 max-w-2xl">
    <h1 class="d1 text-center">{{ page.title }}</h1>
    {% if page.content != blank %}
      <div class="rte pt-10">
        {{ page.content }}
      </div>
    {% endif %}
  </div>
</div>

{% schema %}
  {
    "name": "Page",
    "tag": "section",
    "class": "page"
  }
{% endschema %}